{
  "name": "rxjs-pipe-ext",
  "version": "3.0.0",
  "description": "RxJS pipe extensions",
  "main": "lib/index.js",
  "types": "lib/index.t.ts",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aerabi/rxjs-pipe-ext.git"
  },
  "keywords": [
    "rxjs",
    "observable"
  ],
  "author": "aerabi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aerabi/rxjs-pipe-ext/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/aerabi/rxjs-pipe-ext#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.5.5",
    "jasmine": "^3.5.0",
    "rxjs": "^6.6.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "rxjs": "^6.6.3"
  },
  "prettier": {
    "parser": "typescript",
    "trailingComma": "all",
    "printWidth": 140,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
